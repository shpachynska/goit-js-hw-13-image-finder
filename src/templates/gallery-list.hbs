<ul class='gallery js-results-container'>
  {{#each this}}
    <li>
      <div class='photo-card'>
        <img src='{{webformatURL}}' alt='{{tags}}' />

        <div class='stats'>
          <p class='stats-item'>likes:
            {{likes}}
            <i class='material-icons'>star rate</i>

          </p>
          <p class='stats-item'>views:
            {{views}}
            <i class='material-icons'>face</i>
          </p>
          <p class='stats-item'>comments:
            {{comments}}
            <i class='material-icons'>mode</i>
          </p>
          <p class='stats-item'>downloads:
            {{downloads}}
            <i class='material-icons'>download</i>
          </p>
        </div>
      </div>
    </li>
  {{/each}}
</ul>